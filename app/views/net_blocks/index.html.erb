<h1>Listing net_blocks</h1>

<table>
  <tr>
    <th>Range</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @net_blocks.each do |net_block| %>
  <tr>
    <td><%= net_block.range %></td>
    <td><%= link_to 'Show', net_block %></td>
    <td><%= link_to 'Edit', edit_net_block_path(net_block) %></td>
    <td><%= link_to 'Destroy', net_block, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>


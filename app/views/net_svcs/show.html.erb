<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @net_svc.name %>
</p>

<p>
  <b>Fingerprint:</b>
  <%= @net_svc.fingerprint %>
</p>

<p>
  <b>Port:</b>
  <%= @net_svc.port_num %>
</p>

<p>
<b>Parents:</b>
  <ul>
  <% @net_svc.parents.each {|p|%>
    <li><%= link_to p, eval("#{p.underscore}_path(p)")  %></li>
  <% } %>
  </ul>
</p>  

<p>
<b>Children:</b>
  <ul>
  <% @net_svc.children.each {|c|%>
    <li><%= link_to c, eval("#{c.underscore}_path(c)")  %></li>
  <% } %>
  </ul>
</p>

<p>
<b>Tasks:</b>
  <ul>
  <% @net_svc.tasks.each {|t|%>
    <li><%= link_to t, eval("#{t.underscore}_path(t.name)")  %></li>
  <% } %>
  </ul>
</p>


<%= link_to 'Edit', edit_net_svc_path(@net_svc) %> |
<%= link_to 'Back', net_svcs_path %>

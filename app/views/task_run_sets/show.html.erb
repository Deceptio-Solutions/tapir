<p id="notice"><%= notice %></p>

<table>
  <tr>
    <th>Task Name:</th>
    <th>Result:</th>
    <th>Task Log:</th>
  </tr>
  <% @task_run_set.task_runs.each do |task_run| %>
  <tr>
    <td><%= task_run.task_name %></td>
    <td>
      <ul>
      <% task_run.entity_mappings.each do |x| %>
        <% child = x.get_child 
          if child %>
        <li><%= link_to(child,entity_path(child)) %></li>
      <%  end 
        end %>
      </ul>
    </td>
    <td><%= simple_format task_run.task_log %></td>
  </tr>
  <% end %>
</table>

<%= link_to 'Back', entities_path %>

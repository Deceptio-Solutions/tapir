
<b>Task:</b><br/>
<%= @task %>

<table>
  <tr>
    <th>Input</th>
  </tr>
  <% @objects.each do |o| %>
  <tr>
    <td><a href="<%=request.url%>/../../<%=o.underscore%>s/<%=o.id%>"> <%=o.to_s%></a><br/></td>
  </tr>
  <% end %>
</table>

<table>
  <tr>
    <th>Output</th>
  </tr>
  <% @results.each do |o| %>
  <tr>
    <td><a href="<%=request.url%>/../../<%=o.underscore%>s/<%=o.id%>"> <%=o.to_s%></a><br/></td>
  </tr>
  <% end %>
</table>

<br/>
<b>Task Log:</b><br/>
<%= @task.task_logger.out.string.gsub("\n","<br/>").html_safe %>
<br/>
